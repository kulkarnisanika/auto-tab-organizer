{
  "manifest_version": 3,
  "name": "Auto Tab Grouper",
  "version": "1.0",
  "description": "Automatically groups tabs by domain",
  "permissions": ["tabs", "tabGroups", "scripting"],
  "action": {
    "default_title": "Group Tabs by Domain",
    "default_popup": "./index.html"
  },
  "icons": {
    "16": "./icon32.png",
    "48": "./icon32.png",
    "128": "./icon32.png"
  }
}